    "master.geometry(\"1500x1000\",)\n",
    "master.configure(bg='slate blue')\n",
    "page = Text(master,height = 30,width = 70,bg = \"white\",foreground='blue',wrap='word',spacing2=8,spacing3=8)\n",
    "page.place(x=80,y=80,anchor = NW)\n",
    "mainloop()"
